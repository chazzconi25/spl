{
  "name": "spl",
  "displayName": "SPL Syntax Highlighting",
  "description": "Syntax Highlighting Support for USNA SI413 SPL language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "spl",
      "aliases": ["SI413 Programming Language", "spl"],
      "extensions": [".spl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "spl",
      "scopeName": "source.spl",
      "path": "./syntaxes/grammer.json"
    }]
  }
}
